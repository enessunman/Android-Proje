        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.sunman.FoodList" />
        <activity android:name="com.example.sunman.IndexHesaplama" />
        <activity android:name="com.example.sunman.HafizaOyunu" />
        <activity android:name="com.example.sunman.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>